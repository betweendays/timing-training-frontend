<div class="panel panel-default" id="general" style="display: block;" xmlns="http://www.w3.org/1999/html">
    <div class="panel-heading">
        <h3 class="panel-title">{{question}}</h3>
    </div>
    <div class="panel-body">

        <!-- Objectives List -->
        <div id="objectiveList" class="bs-example" data-example-id="simple-list-group" >
            <ul class="list-group">
                {{#each options}}
                    <a href='/trainingSetup/setSpecificOptions/0/{{id}}' class="list-group-item">{{name}}</a>
                {{/each}}
            </ul>
        </div>

        <!--Back button-->
    </br><p><a class="btn btn-primary" href="/home" role="button">Back</a></p>
    </div>
</div>